{
    "name" : "Extension" ,
    "version" : "1.0" ,
    "description" :
        "To fetch json data on clicking chrome extension." ,
    "browser_action":{
    	"default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
          }
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      },
   "background":{
        "scripts":["background.js"],
        "persistent": false
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions":["tabs"],
    "manifest_version" : 2
}